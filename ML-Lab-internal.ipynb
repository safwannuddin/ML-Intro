    "import numpy as np\n",
    "\n",
    "def bubblesort(a):\n",
    "    n=len(a)\n",
    "    for i in range(n):\n",
    "        for j in range(0,n-i-1):\n",
    "        if(a[j] > a[j+1]):\n",
    "            a[j],a[j+1]=a[j+1],a[j]\n",
    "\n",
    "    a= np.array([64,25,12,22,11])\n",
    "    bubblesort(a)\n",
    "    print(a)"
   ]
