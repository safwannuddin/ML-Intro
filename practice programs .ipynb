   "source": [
    "import numpy as np \n",
    "def bubble_sort(a):\n",
    "    n=len(a)\n",
    "    for i in range(n):\n",
    "        for j in range(n-i-1):\n",
    "            if (a[j] > a[j+1]):\n",
    "                a[j],a[j+1]=a[j"
   ]
